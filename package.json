{
  "name": "restful-booking-api-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engine": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vitest --reporter=allure-vitest/reporter --reporter=html",
    "test:run": "vitest run --reporter=allure-vitest/reporter",
    "test:generate-report": "npx allure generate allure-results -o allure-report --clean",
    "test-windows:copy-history": "robocopy allure-report/history allure-results/history > NUL",
    "test-linux:copy-history": "cp -r allure-report/history/. allure-results/history/ > /dev/null 2&1",
    "test:open-report": "npx allure open allure-report",
    "test-windows": "npm run test:run && npm run test:generate-report && npm run test-windows:copy-history & npm run test:open-report",
    "test-linux": "npm run test:run && npm run test:generate-report && npm run test-linux:copy-history & npm run test:open-report",
    "clean": "rmdir /s /q allure-results allure-report html"
  },
  "author": "Thinh Luu",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@vitest/ui": "^2.0.5",
    "allure-commandline": "^2.30.0",
    "allure-vitest": "^3.0.0-beta.10",
    "dotenv": "^16.4.5",
    "export-to-csv": "^1.3.0",
    "formdata-to-string": "^2.0.2",
    "lodash": "^4.17.21",
    "object-to-formdata": "^4.5.1",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "restful-booking-api-test": "file:"
  }
}
